	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-sveltekit": "^0.10.1",
		"@supabase/supabase-js": "^2.24.0",
		"ai": "^2.1.28",
		"dotenv": "^16.3.1",
		"highlight.js": "^11.8.0",
		"langchain": "^0.0.102",
		"nanoid": "^4.0.2",
		"openai-edge": "^1.2.2",
		"remark-parse": "^10.0.2",
		"stripe": "^12.18.0",
		"unified": "^10.1.2"
	}
